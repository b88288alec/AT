<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class catalog="TFHK" name="com.tfhk.cfgparam.model.CfgParam" table="cfg_param">
  <id name="code" type="string">
   <column length="50" name="CODE"/>
   <generator class="assigned"/>
  </id>
  <property generated="never" lazy="false" name="codeName" type="string">
   <column length="50" name="CODE_NAME"/>
  </property>
  <property generated="never" lazy="false" name="codeValue" type="string">
   <column length="50" name="CODE_VALUE"/>
  </property>
  <property generated="never" lazy="false" name="codeDesc" type="string">
   <column length="50" name="CODE_DESC"/>
  </property>
 </class>
</hibernate-mapping>
